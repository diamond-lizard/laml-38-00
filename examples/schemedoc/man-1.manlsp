(((title "My Manual") (author "My Name") (affiliation "My affiliation") (abstract "Some abstract. As an example we extract\n                      interface documention from the color library.") (scheme-source-file . "/user/normark/scheme/lib/color.scm/") (laml-resource . "false") (documentation-commenting-style . "documentation-mark") (default-attribute-explanation . not-provided) (source-destination-delta . not-provided) (css-prestylesheet . "compact") (css-stylesheet . "original") (css-stylesheet-copying . "true") (make-dtd-manual-template . not-provided) (keep-syntactical-comment-file . not-provided) (manual-destination-name . not-provided) (mirror-names-defined . not-provided) (mirror-name-prefix . not-provided) (attribute-sorting . not-provided) (scheme-source-linking . not-provided)) ((kind "manual-section") (section-title "Primary color encoding function.") (section-body "The function in this section, rgb-color-encoding, accepts a variety of different color formats as input. It returns a string of length seven, such as \"#ff00ff\". The output format is the primary color representation in most web contexts. ") (section-id "primary-function")) ((kind "manual-page") (cross-references (internal-references "more efficient functions" "rgb-color" "rgb-color-list")) (description "Return a color encoding (a string of length seven such as \"#123456\") of color-pars. The color-pars parameter(s) to this function are very versatile.<p> If it is a color encoding string already, just return it.<br> If it is a color value which satisfies the predicate rgb-color?, return the encoding of this value. <br> If it is a symbol, return the color encoding bound to it. <br> If it is a string, transform it to a symbol and return the color encoding bound to it. <br> If it is a list like (list 1 2 3), consider it as a red-green-blue list, and return the color encoding of it. <br> If it is three individiual parameters, say r, g, and b, return the color encoding of red r, green g, and blue b. <p> If you care about efficiency, use can consider to use the function rgb-color instead of rgb-color-encoding. ") (old-example "(rgb-color-encoding \"red\")") (returns "A string of length 7 of the format \"#rrggbb\".") (note "This function works with both the new and old representation of colors in LAML.") (title "rgb-color-encoding") (form (rgb-color-encoding . color-pars))) ((kind "manual-section") (section-title "Secondary color encoding functions.") (section-body " The functions in this section only work with the old version of the color representation. This is the untagged list representation, such as '(255 0 0). <p> For new development, the function make-rgb-color should be used together with the color encoding function rgb-color-encoding.")) ((kind "manual-page") (parameters (parameter "r" "The amount of red - a decimal number between 0 and 255. ") (parameter "g" "The amount of green - a decimal number between 0 and 255. ") (parameter "b" "The amount of blue - a decimal number between 0 and 255. ")) (description "Return an 'Internet color string\" encoding the colors r, g, and b. ") (form "(rgb-color r g b)") (returns "A string of length 7 of the form \"#rrggbb\".") (title "rgb-color")) ((kind "manual-page") (parameters (parameter "color-triple-list" "A list of length 3. Each element of the list is a decimal integer between 0 and 255.")) (description "Returns the color encoding of (list r g b) given a list of three color numbers as parameter. ") (form "(rgb-color-list color-triple-list)") (returns "A string of length 7 of the form \"#rrggbb\".") (title "rgb-color-list")) ((kind "manual-section") (section-title "Color constructor, predicate, and selectors.") (section-body "The function make-rgb-color is the primary color constructor in LAML-based software. The predidate and the selectors only work with make-rgb-color. The function make-color is an old version of the constructor. ")) ((kind "manual-page") (parameters (parameter "r" "The amount of red - a decimal number between 0 and 255. ") (parameter "g" "The amount of green - a decimal number between 0 and 255. ") (parameter "b" "The amount of blue - a decimal number between 0 and 255. ")) (cross-references (internal-references "conversion function" "rgb-color-encoding")) (description "Make and return a color represented by a red, green and blue constituent. This is the primary color constructor of LAML-based software. ") (returns "A tagged list of color numbers.") (old-example "(make-rgb-color 255 0 0) => (rgb-color 255 0 0)") (title "make-rgb-color") (form (make-rgb-color r g b))) ((kind "manual-page") (description "Is x a LAML color ") (title "rgb-color?") (form (rgb-color? x))) ((kind "manual-page") (parameters (parameter "color" "A color constructed with make-rgb-color.")) (cross-references (internal-references "color constructor" "make-rgb-color")) (description "Return the red constituent of the LAML color. ") (form "(red-of-rgb-color color)") (title "red-of-rgb-color")) ((kind "manual-page") (parameters (parameter "color" "A color constructed with make-rgb-color.")) (cross-references (internal-references "color constructor" "make-rgb-color")) (description "Return the green constituent of the color. ") (form "(green-of-rgb-color color)") (title "green-of-rgb-color")) ((kind "manual-page") (parameters (parameter "color" "A color constructed with make-rgb-color.")) (cross-references (internal-references "color constructor" "make-rgb-color")) (description "Return the blue constituent of the color. ") (form "(blue-of-rgb-color color)") (title "blue-of-rgb-color")) ((kind "manual-page") (description "Make and return the rgb-list representation of the color with r red, g green, and b blue. This is an old version of the color contructor. ") (misc "Deprecated. Use make-rgb-color.") (title "make-color") (form (make-color r g b))) ((kind "manual-section") (section-title "Color constants.") (section-body "To stay backward compatible with a substantial amount of older LAML software, all color constants are bound to the old LAML color representation. Thus, for instance, the value of red is (255 0 0), and not (rgb-color 255 0 0). As an important observation, the primary color encoding function, rgb-color-encoding, accepts the value of the color constants as input (besides a number of other kinds of input).")) ((kind "manual-page") (description "A color constant. A color is represented as a list of integers of length three (rgb). ") (title "red") (form red)) ((kind "manual-page") (description "A color constant. A color is represented as a list of integers of length three (rgb). ") (title "dark-red") (form dark-red)) ((kind "manual-page") (description "A color constant. A color is represented as a list of integers of length three (rgb). ") (title "green") (form green)) ((kind "manual-page") (description "A color constant. A color is represented as a list of integers of length three (rgb). ") (title "green1") (form green1)) ((kind "manual-page") (description "A color constant. A color is represented as a list of integers of length three (rgb). ") (title "green2") (form green2)) ((kind "manual-page") (description "A color constant. A color is represented as a list of integers of length three (rgb). ") (title "blue") (form blue)) ((kind "manual-page") (description "A color constant. A color is represented as a list of integers of length three (rgb). ") (title "white") (form white)) ((kind "manual-page") (description "A color constant. A color is represented as a list of integers of length three (rgb). ") (title "black") (form black)) ((kind "manual-page") (description "A color constant. A color is represented as a list of integers of length three (rgb). ") (title "yellow") (form yellow)) ((kind "manual-page") (description "A color constant. A color is represented as a list of integers of length three (rgb). ") (title "purple") (form purple)) ((kind "manual-page") (description "A color constant. A color is represented as a list of integers
(define while-flow-graph
 (with-xml-language! svg-language '(filter)
  (let* ((x0 100) (x1 200) (x2 500)
	 (y0 100) (y1 200) (y2 300)
        )
   (with-animation '(step-buttons-walk-through-edge-motion) ; none  step-buttons-reveal  step-buttons-walk-through
    (let*
        ((start-node (empty-svg-node x0 y0))
         (join-delta 60)
         (edge-text-props '(font-size 22))
         (end-node (empty-svg-node x0 y2))
         (expr-node (svg-node diamond x1 y1 "Logical Expression" 'font-size 25 'min-width 270 'min-height 70 'steps "2,6,10"))
         (command-node (svg-node cloud x2 y1 "Command" 'font-size 25 'min-width 150 'min-height 70 'steps "4,8"))
         (c1 (empty-svg-node (+ x2 120) y1)) (c2 (empty-svg-node (+ x2 120) (- y1 80))) (c3 (empty-svg-node x1 (- y1 80)))  

         (start-edge (svg-edge start-node "cc" expr-node "ct" 'style "hv" 'stroke "black" 'arrow "yes" 'steps "1" "Entrance" edge-text-props 'ldx -35 'ldy -50))
         (end-edge (svg-edge expr-node "cb" end-node "cc" 'style "vh" 'stroke "black" 'arrow "yes" 'steps "11" "false" edge-text-props 'font-style "italic" 'ldy -10))
         (true-edge (svg-edge expr-node "rc" command-node "lc" 'stroke-width "1" 'stroke "black" 'arrow "yes" 'steps "3,7" "true" edge-text-props 'font-style "italic" 'ldx -20 'ldy -10))
         (repeat-edge (svg-edge-broken command-node "rc" expr-node "ct" (list c1 c2 c3) 'stroke-width "1" 'stroke "black" 'arrow "yes" 'steps "5,9"))
	 )
      (svg-graph 'from-step 1 'to-step 11
        (list start-node expr-node command-node end-node)
        (list true-edge start-edge  end-edge repeat-edge)
        (explanations 'font-size 18
          (explanation 'step 1 "We enter the while loop")
          (explanation 'step 2 "The logical expression is evaluated")
          (explanation 'step 3 "In this case the value of the expression is true...")
          (explanation 'step 4 "...  meaning that the while body command is executed.")
          (explanation 'step 5 "When done the control is brought back the logical expression")
          (explanation 'step 6 "... which now is RE-evaluated. Variables in the logical expressions may have been changed")
          (explanation 'step 7 "Let us assume that we reiterate ...")
          (explanation 'step 8 "... through the command yet another time ...")
          (explanation 'step 9 "... and back again to the controlling logical expresssion")
          (explanation 'step 10 "... which is RE-evaluated a third time.")
          (explanation 'step 11 "Now the expression becomes false. The while loop has run to the end.")
        )

       )
      )
    ))))


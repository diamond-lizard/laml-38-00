         (se (lambda (n) (- (* n 2) 1)))
         (motion-edge-down (lambda (fn tn via-nodes time) (svg-edge-broken fn "cb" tn "ct" via-nodes 'stroke-width "0" 'step (as-string time))))
         (motion-edge-up (lambda (fn tn via-nodes time) (svg-edge-broken fn "ct" tn "cb" via-nodes 'stroke-width "0" 'step (as-string time))))

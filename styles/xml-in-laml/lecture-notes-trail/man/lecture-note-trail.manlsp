(((title . "Lecture Note Trail Manual") (author . "Kurt N\370rmark    normark@cs.aau.dk") (affiliation . "Department of Computer Science, Aalborg University, Denmark.") (abstract . "This is a manual of the LENO Trail facility. Using the LENO trail\n           facility it is possible to select a subset of slides in a lecture,\n           and to generate a collection of interlinked frameset pages. The best way to initiate a new trail is to ask LENO to generate a complete trail (on a separate, secondary\n           source file).\n           and then delete the page-select clauses which you do not need.\n           The generated trail covers all pages in all lectures. The attribute trail-source \n           in the leno-front-matters element controls the trail generation. You can of course also\n           reorganize the page-select clauses. You may consult section 4 of the LENO Tutorial to learn more about LENO trails. Similar resources: The primary LENO language and The Theme LENO language.") (scheme-source-file . not-provided) (laml-resource . "true") (documentation-commenting-style . not-provided) (default-attribute-explanation . not-provided) (source-destination-delta . not-provided) (css-prestylesheet . "compact") (css-stylesheet . "argentina") (css-stylesheet-copying . "true") (make-dtd-manual-template . not-provided) (keep-syntactical-comment-file . not-provided) (manual-destination-name . not-provided) (mirror-names-defined . not-provided) (mirror-name-prefix . not-provided) (attribute-sorting . not-provided) (scheme-source-linking . not-provided)) ((kind "manual-page") (title "leno-trail") (description "The root element of the LENO trail source file.") (attribute-descriptions) (cross-references) (kind "manual-page") (content-model "(leno-trail-front-matters,begin-trail,page-select*,end-trail)") (attributes)) ((kind "manual-page") (title "leno-trail-front-matters") (description "The front matters elements holds a number of overall attributes of the LENO trail.") (attribute-descriptions (attribute-description "trail-id" "The unique id of this trail. The id determines the prefix of the names of the generated HTML files.") (attribute-description "scheme-prefix" "The scheme prefix file. Similar to the leno-front-matters attribute of the same name") (attribute-description "scheme-suffix" "The scheme suffix file. Similar to the leno-front-matters attribute of the same name") (attribute-description "course-home-url" "The URL of the designated course home page. Similar to the leno-front-matters attribute of the same name") (attribute-description "language" "Controls the language of the fixed text")) (cross-references) (kind "manual-page") (content-model "(front-title,front-author,front-affiliation,front-abstract)") (attributes (attribute "trail-id" "CDATA" "#REQUIRED") (attribute "scheme-prefix" "CDATA" "#IMPLIED") (attribute "scheme-suffix" "CDATA" "#IMPLIED") (attribute "course-home-url" "CDATA" "#REQUIRED") (attribute "language" ("danish" "english") "english")) (cross-references (internal-references "enclosing element" "leno-trail"))) ((kind "manual-page") (title "front-title") (description "The title of the LENO trail") (attribute-descriptions) (cross-references) (kind "manual-page") (content-model "(#PCDATA)") (attributes) (cross-references (internal-references "enclosing element" "leno-trail-front-matters"))) ((kind "manual-page") (title "front-author") (description "The author of the LENO trail") (attribute-descriptions) (cross-references) (kind "manual-page") (content-model "(#PCDATA)") (attributes) (cross-references (internal-references "enclosing element" "leno-trail-front-matters"))) ((kind "manual-page") (title "front-affiliation") (description "The affiliation of the trail author") (attribute-descriptions) (cross-references) (kind "manual-page") (content-model "(#PCDATA)") (attributes) (cross-references (internal-references "enclosing element" "leno-trail-front-matters"))) ((kind "manual-page") (title "front-abstract") (description "The abstract of the LENO trail") (attribute-descriptions) (cross-references) (kind "manual-page") (content-model "(#PCDATA)") (attributes) (cross-references (internal-references "enclosing element" "leno-trail-front-matters"))) ((kind "manual-page") (title "begin-trail") (description "An empty element before the first page-select clause.") (attribute-descriptions) (cross-references) (kind "manual-page") (content-model "EMPTY") (attributes) (cross-references (internal-references "enclosing element" "leno-trail"))) ((kind "manual-page") (title "end-trail") (description "An empty element after the last page-select clause") (attribute-descriptions) (cross-references) (kind "manual-page") (content-model "EMPTY") (attributes) (cross-references (internal-references "enclosing element" "leno-trail"))) ((kind "manual-page") (title "page-select") (description "Selects a page of a LENO lecture. This is the most important element of a LENO trail source file. A sequence of page-select elements in between begin-trail and end-trail constitute the body of a LENO trail. ") (attribute-descriptions (attribute-description "lecture-id" "The id of a LENO lecture. Must be located in the same lecture as this trail file.") (attribute-description "page-id" "The page id of a page in the lecture addressed by lecture-id")) (cross-references) (kind "manual-page") (content-model "EMPTY") (attributes (attribute "lecture-id" "CDATA" "#REQUIRED") (attribute "page-id" "CDATA" "#REQUIRED")) (cross-references (internal-references "enclosing element" "leno-trail"))))